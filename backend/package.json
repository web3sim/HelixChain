{
  "name": "genomic-privacy-backend",
  "version": "1.0.0",
  "description": "Backend API for Genomic Privacy DApp on Midnight Blockchain",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "migrate": "node-pg-migrate",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "migrate:create": "node-pg-migrate create",
    "queue:worker": "ts-node src/proof/proof.worker.ts",
    "seed:demo": "ts-node -r tsconfig-paths/register scripts/seed-demo-database.ts",
    "generate:demo": "ts-node scripts/generate-demo-data.ts",
    "start:all": "npm run seed:demo && npm run dev"
  },
  "dependencies": {
    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^2.0.1",
    "@types/form-data": "^2.2.1",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "bull": "^4.11.4",
    "circomlib": "^2.0.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.15.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.4",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "snarkjs": "^0.7.5",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.4",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "node-pg-migrate": "^6.2.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "genomic",
    "privacy",
    "midnight",
    "blockchain",
    "zero-knowledge"
  ],
  "author": "Team Genomic Privacy",
  "license": "MIT"
}
